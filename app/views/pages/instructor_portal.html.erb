<h1> Instructor Portal</h1>
<div>
  <%= form_tag("/instructor/create", method: "post") do %>
    <%= text_field(:instructor, :first_name, placeholder: "First Name") %>
    <%= text_field(:instructor, :last_name, placeholder: "Last Name") %>
    <%= text_field(:instructor, :age, placeholder: "age") %>
  <% end %>
</div>
<br>
<div>
  <table class="primary">
    <thead>
      <tr>
        
        <th>First Name</th>
        <th> Last Name</th>
        <th>Age</th>
      </tr>
    </thead>
    <tbody>
      <% @all_instructors.each do |instructors| %>
      <tr>
        <td><%= instructors.first_name %></td>
        <td><%= instructors.last_name %></td>
        <td><%= instructors.age %></td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>